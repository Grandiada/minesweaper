{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"whitespace": true,
		"eofline": false,
		"no-trailing-whitespace": false,
		"comment-format": false,
		"quotemark": false,
		"no-console": false,
		"one-line": false,
		"no-consecutive-blank-lines": false,
    "curly": false,
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false
	},
	"rulesDirectory": []
}
